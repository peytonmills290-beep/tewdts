local combatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})





combatTab:AddDropdown({
    Name = "Combat Modifications",
    Default = "Select Option",
    Options = CombatMods,
    Callback = function(selected)
        -- Reference your modification table here
        -- Example structure (you need your game's variables)
        local Modifications = {
            InfiniteAmmo = false,
            InfiniteClips = false,
            InfiniteDamage = false,
            ModifyFireRate = false,
            ModifySpread = false,
            ModifyRecoil = false,
            Automatic = false,
            ModifyReloadSpeed = false,
            ModifyEquipSpeed = false
        }

        -- Toggle the selected mod
        Modifications[selected] = not Modifications[selected]
        print(selected .. " set to", Modifications[selected])

        -- You can connect these toggles to actual game logic here
        -- Example (if the game has a remote or stat to fire)
        -- game:GetService("ReplicatedStorage").Remote.Modifications:FireServer(Modifications)
    end
})





local auraTab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})






auraTab:AddDropdown({
    Name = "Player / Combat Mods",
    Default = "Select Option",
    Options = {"ESP", "WalkSpeed", "SuperJump", "KillAura"},
    Callback = function(selected)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")

        if selected == "ESP" then
            -- Basic ESP toggle example
            for _, plr in pairs(game.Players:GetPlayers()) do
                if plr ~= player then
                    local box = Instance.new("BoxHandleAdornment")
                    box.Adornee = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
                    box.Size = Vector3.new(2, 5, 1)
                    box.Color3 = Color3.fromRGB(0, 255, 0)
                    box.Transparency = 0.5
                    box.Parent = workspace
                end
            end
            print("ESP enabled (simple example)")

        elseif selected == "WalkSpeed" then
            if humanoid then
                humanoid.WalkSpeed = 40
                print("WalkSpeed set to 40")
            end

        elseif selected == "SuperJump" then
            if humanoid then
                humanoid.JumpPower = 40
                print("JumpPower set to 40")
            end

        elseif selected == "KillAura" then
            local range = 100
            local runService = game:GetService("RunService")
            
            -- Simple loop to damage nearby players
            local connection
            connection = runService.Heartbeat:Connect(function()
                for _, plr in pairs(game.Players:GetPlayers()) do
                    if plr ~= player and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                        local hrp = plr.Character.HumanoidRootPart
                        local dist = (hrp.Position - character.HumanoidRootPart.Position).Magnitude
                        -- Check distance and if holding a gun (example check)
                        local tool = character:FindFirstChildOfClass("Tool")
                        if dist <= range and tool and tool:IsA("Tool") then
                            local hum = plr.Character:FindFirstChildOfClass("Humanoid")
                            if hum and hum.Health > 0 then
                                hum.Health = 0
                            end
                        end
                    end
                end
            end)
            print("Kill Aura enabled (range 100, requires gun)")
        end
    end
})








OrionLib:Init()
